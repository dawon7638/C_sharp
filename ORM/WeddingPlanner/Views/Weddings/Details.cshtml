@model Wedding
@{
    int? uid = @Context.Session.GetInt32("Userid");
}
<div class="text-center row justify-content-around align-items-center">
    <h3> @Model.CouplesName()'s Wedding</h3>
    <a href="/Dashboard">Dashboard</a>
    <a href="/">Log Out (@Context.Session.GetString("FullName"))</a>
</div>

<h5 class="m-3">Date: @Model.Date.ToString("MMMM d, yyyy")</h5>
<h5>Guests:</h5>
<ul class="list-unstyled">
@foreach (Guest rsvp in Model.RSVPs)
{
    <li>@rsvp.User.FullName()</li>
}
</ul>



