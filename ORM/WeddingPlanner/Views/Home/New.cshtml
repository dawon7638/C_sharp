@model Wedding
<div class="text-center row justify-content-around">
    <h1>Plan Your Wedding </h1>
    <a href="/Dashboard">Dashboard</a>
    <a href="/">Log Out</a>
</div>

<form asp-action="Create" asp-controller="Home" method="POST">
    <div class="form-group">
        <label asp-for="WedderName1"></label>
        <span asp-validation-for="WedderName1" class="text-danger"></span>
        <input asp-for="WedderName1" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="WedderName2"></label>
        <span asp-validation-for="WedderName2" class="text-danger"></span>
        <input asp-for="WedderName2" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Date"></label>
        <span asp-validation-for="Date" class="text-danger"></span>
        <input asp-for="Date" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Address"></label>
        <span asp-validation-for="Address" class="text-danger"></span>
        <input asp-for="Address" class="form-control">
    </div>
    <button type="submit" class="btn btn-outline-success">Create</button>
</form>