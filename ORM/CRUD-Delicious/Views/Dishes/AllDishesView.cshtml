@model List<DishModel>


<div class="container">
    <a href="/new">Add a Dish</a>
    <h3 class="text-center">
        Welcome to CRUDelicious
    </h3>
    <h5>Check out some recent dishes!</h5>
    <hr>
</div>
@foreach (DishModel dish in Model)
{
    <p><a asp-controller="Dishes" asp-action="ReadView" asp-route-dishId="@dish.DishId">@dish.Name</a> by @dish.Chef</p>
}