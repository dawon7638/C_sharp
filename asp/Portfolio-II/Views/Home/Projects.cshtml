        "Cool thing 5!",
        "Cool thing 6!"
        };
            foreach (string project in StringList)
            {
                <ul>
                    <div class="project-stuff">
                        <div class="project-pic">
                            placeholder
                        </div>
                        <div>
                            <p class="project-items">@project</p>
                            <p>Lorem ipsum dolor, sit amet consectetur adipisicing elit. Maiores possimus voluptas, dolorum
                                laboriosam debitis velit iusto quibusdam unde aut, corrupti voluptate similique modi enim id
